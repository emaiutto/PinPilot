<UserControl x:Class="MauiSoft.SRP.Gauges.Generics.GPS"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MauiSoft.SRP.Gauges.Generics"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">

    <Grid Style="{DynamicResource GridStyle1}" Background="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}">
        <Label Style="{DynamicResource  LabelStyleNumbers }" x:Name="Lat" Content="0.00 00" HorizontalAlignment="Center" Margin="0,-35,0,0" VerticalAlignment="Center" />
        <Label Style="{DynamicResource LabelStyleNumbers }" x:Name="Lon" Content="0.00 00" HorizontalAlignment="Center" Margin="0,35,0,0" VerticalAlignment="Center" />
    </Grid>

</UserControl>
